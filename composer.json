{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "barryvdh/laravel-ide-helper": "^2.6.7",
        "diglactic/laravel-breadcrumbs": "^6.1",
        "doctrine/dbal": "^2.8",
        "fakerphp/faker": "^1.9.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "garygreen/pretty-routes": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.4.4",
        "laravel/framework": "^8.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "~6.2.0",
        "maatwebsite/excel": "~3.1",
        "renatomarinho/laravel-page-speed": "^2.0",
        "sentry/sentry-laravel": "^2.4",
        "spatie/calendar-links": "^1.4",
        "spatie/flysystem-dropbox": "^1.2",
        "spatie/icalendar-generator": "^2.0",
        "spatie/laravel-backup": "^7.1",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-query-builder": "^4.0",
        "stancl/tenancy": "^3.1",
        "tightenco/ziggy": "^1.3",
        "webklex/laravel-imap": "^1.4"
    },
    "type": "project",
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.3",
        "beyondcode/laravel-query-detector": "^1.5",
        "brianium/paratest": "^6.1",
        "facade/ignition": "^2.3.6",
        "jasonmccreary/laravel-test-assertions": "^2.0",
        "laravel-shift/factory-generator": "dev-master",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.7.0",
        "phpunit/php-code-coverage": "^9.2",
        "phpunit/phpunit": "^9.0",
        "wnx/laravel-stats": "^2.2"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan telescope:publish"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
